{
  "$schema": "https://schema.railpack.com",
  "provider": "node",
  "packages": {
    "nodejs": "20"
  },
  "steps": {
    "install": {
      "commands": [
        "npm ci"
      ]
    },
    "build": {
      "commands": [
        "npm run build"
      ]
    }
  },
  "deploy": {
    "startCommand": "npm start"
  }
}
